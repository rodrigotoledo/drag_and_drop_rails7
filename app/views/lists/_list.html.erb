<%=turbo_frame_tag dom_id(list), data: { sgid: list.to_sgid_param } do %>
  <div class="border border-1 mb-2 p-3 turbo-content">
    <div class="btn-group" role="group" aria-label="Actions">
      <%= link_to "Edit", edit_list_path(list), class: 'btn btn-primary' %>
      <%= link_to "Show", list, class: 'btn btn-secondary', data: { turbo: false } %>
    </div> <%= list.name %>
  </div>
<% end %>